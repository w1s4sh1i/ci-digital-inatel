    %parti/s 1, 5, 4;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_1.4, 4;
    %load/vec4 v0x5b4450b7ff40_0;
    %addi 1, 0, 6;
    %assign/vec4 v0x5b4450b7ff40_0, 0;
T_1.4 ;
T_1.3 ;
