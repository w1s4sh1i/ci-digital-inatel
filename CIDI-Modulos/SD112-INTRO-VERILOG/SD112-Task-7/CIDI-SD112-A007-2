    %load/vec4 v0x5b1273b07560_0;
    %cmp/ne;
    %jmp/0xz  T_0.0, 4;
    %load/vec4 v0x5b1273b073b0_0;
    %addi 1, 0, 6;
    %assign/vec4 v0x5b1273b073b0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x5b1273b073b0_0, 0;
T_0.1 ;
    %load/vec4 v0x5b1273b073b0_0;
    %cmpi/e 63, 0, 6;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x5b1273b07670_0;
    %parti/s 1, 1, 2;
    %assign/vec4 v0x5b1273b07560_0, 0;
T_0.2 ;
    %load/vec4 v0x5b1273b07560_0;
    %assign/vec4 v0x5b1273adfdd0_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5b1273af07a0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b1273b07870_0, 0, 1;
T_1.0 ;
    %delay 10, 0;
    %load/vec4 v0x5b1273b07870_0;
    %inv;
    %store/vec4 v0x5b1273b07870_0, 0, 1;
    %jmp T_1.0;
    %end;
