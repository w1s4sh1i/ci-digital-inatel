:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
S_0x5661b7116ad0 .scope module, "mux_8x1_tb" "mux_8x1_tb" 2 17;
 .timescale -9 -12;
P_0x5661b7113a10 .param/l "DELAY" 1 2 19, +C4<00000000000000000000000000001010>;
P_0x5661b7113a50 .param/l "WIDTH" 1 2 19, +C4<00000000000000000000000000001000>;
v0x5661b713cb50_0 .var "in", 7 0;
v0x5661b713cc60_0 .net "out", 0 0, L_0x5661b713ce30;  1 drivers
v0x5661b713cd30_0 .var "sel", 2 0;
S_0x5661b70eee90 .scope module, "U1" "mux_8x1" 2 24, 3 17 0, S_0x5661b7116ad0;
 .timescale -9 -12;
    .port_info 0 /INPUT 8 "in";
    .port_info 1 /INPUT 3 "sel";
    .port_info 2 /OUTPUT 1 "out";
v0x5661b70ef0c0_0 .net "in", 7 0, v0x5661b713cb50_0;  1 drivers
v0x5661b713c660_0 .net "out", 0 0, L_0x5661b713ce30;  alias, 1 drivers
v0x5661b713c720_0 .net "sel", 2 0, v0x5661b713cd30_0;  1 drivers
L_0x5661b713ce30 .part/v v0x5661b713cb50_0, v0x5661b713cd30_0, 1;
S_0x5661b713c890 .scope task, "mux" "mux" 2 30, 2 30 0, S_0x5661b7116ad0;
 .timescale -9 -12;
v0x5661b713ca70_0 .var/i "i", 31 0;
TD_mux_8x1_tb.mux ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5661b713ca70_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x5661b713ca70_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.1, 5;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x5661b713cb50_0, 0, 8;
    %load/vec4 v0x5661b713ca70_0;
    %pad/s 3;
    %store/vec4 v0x5661b713cd30_0, 0, 3;
    %pushi/vec4 1, 0, 1;
    %ix/getv 4, v0x5661b713cd30_0;
    %store/vec4 v0x5661b713cb50_0, 4, 1;
    %delay 10000, 0;
    %load/vec4 v0x5661b713ca70_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5661b713ca70_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .scope S_0x5661b7116ad0;
T_1 ;
    %vpi_call 2 54 "$dumpfile", "CIDI-SD112-A014-2-mx81.vcd" {0 0 0};
    %vpi_call 2 55 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5661b7116ad0 {0 0 0};
    %vpi_call 2 57 "$display", "|Input\011 |Select|Ouput  |" {0 0 0};
    %vpi_call 2 58 "$monitor", "|%b|%b\011|%b\011|", v0x5661b713cb50_0, v0x5661b713cd30_0, v0x5661b713cc60_0 {0 0 0};
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x5661b713cd30_0, 0, 3;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x5661b713cb50_0, 0, 8;
    %delay 10000, 0;
    %fork TD_mux_8x1_tb.mux, S_0x5661b713c890;
    %join;
    %delay 10000, 0;
    %vpi_call 2 67 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
